<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="appProgressDialog" parent="android:Theme.Translucent.NoTitleBar">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <declare-styleable name="NNAcresProgressWheel">
        <attr name="matProg_progressIndeterminate" format="boolean" />
        <attr name="matProg_barColor" format="color" />
        <attr name="matProg_rimColor" format="color" />
        <attr name="matProg_rimWidth" format="dimension" />
        <attr name="matProg_spinSpeed" format="float" />
        <attr name="matProg_barSpinCycleTime" format="integer" />
        <attr name="matProg_circleRadius" format="dimension" />
        <attr name="matProg_fillRadius" format="boolean" />
        <attr name="matProg_barWidth" format="dimension" />
        <attr name="matProg_linearProgress" format="boolean" />
    </declare-styleable>
    <style name="MyDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@drawable/rectangle_semitranparent_light_theme</item>

    </style>
    <attr name="item.checkCircle.backgroundColor" format="reference|color"/>
    <attr name="item.checkCircle.borderColor" format="reference|color"/>
    <attr name="bottom_sheet_tint" format="reference" />
    <attr name="textcolorsecondrycustom" format="reference" />
    <attr name="ic_search_vector" format="reference" />
    <attr name="ic_home_vector" format="reference" />
    <attr name="ic_sort_vector" format="reference" />




    <attr name="filemanagertextcolor" format="reference"/>
    <attr name="ic_select_all_vector" format="reference"/>
    <attr name="ic_folder_icon" format="reference"/>
    <attr name="ic_unhide_vector_dialog" format="reference"/>
    <attr name="ic_plus_vector" format="reference"/>


    <attr name="ic_icon_next" format="reference"/>
    <style name="bs_image_style">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:tint">?attr/bottom_sheet_tint</item>
        <item name="android:layout_weight">0</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="bs_textview_style" parent="android:Widget.TextView">
        <item name="android:textSize">@dimen/text_medium</item>
        <!-- <item name="android:tint">?attr/textcolorsecondrycustom</item>-->
        <item name="android:textColor">?attr/textcolorsecondrycustom</item>
        <item name="android:layout_weight">0</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">@dimen/spacing24</item>

    </style>
    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/text_big</item>
<!--
        <item name="fontPath">fonts/OpenSans-SemiBold.ttf</item>
-->
    </style>

    <dimen name="text_medium">14dp</dimen>
</resources>